/*
 * Tooltip styling for apps
 */
.tooltip{
  color:#fff;
  font:15px/1.1 Arial,Georgia,sans-serif;
  font-weight: bold;
  text-shadow: 0 -1px 1px #222;
  position:absolute; 
  padding:5px 10px; 
  top:20px; 
  margin:1em 0 3em;
  background:#075698; 
  left:20px;
  border: solid 1px #ddd;

  -moz-border-radius:4px; 
  -webkit-border-radius:4px; 
  border-radius:4px;   
  background:-webkit-gradient(linear, left top, left bottom, from(rgba(10,10,10,1)), to(rgba(25,25,25,.65)));
  background:-moz-linear-gradient(top,rgba(10,10,10,1),rgba(80,80,80,.9));
  background:-o-linear-gradient(top,rgba(10,10,10,1),rgba(80,80,80,.9));
  background:linear-gradient(top,rgba(10,10,10,1),rgba(80,80,80,.9));

  -moz-box-shadow: 0px 0px 4px #000; /* FF3.5+ */ 
  -webkit-box-shadow: 0px 0px 4px #000; /* Saf3.0+, Chrome */ 
  box-shadow: 0px 0px 4px #000; /* Opera 10.5, IE 9.0 */ 
  filter: progid:DXImageTransform.Microsoft.dropshadow(OffX=0px, OffY=0px, Color='#000'); /* IE6,IE7 */ 
  -ms-filter: "progid:DXImageTransform.Microsoft.dropshadow(OffX=0px, OffY=0px, Color='#000')"; /* IE8 */
  width: 200px;
  z-index: 9999;
}
.tooltip img {
  float: left;
/*  margin: 0 10px 0 0;*/
  margin: 2px 0 0 0;
  width: 20px;
}
.tooltip span.tt_title {
  float: right;
  width: 170px;
}
.tooltip table {
  font-size: 11px;
  margin: 10px 0 0 0;
  width: 100%;
}
.tooltip table td{
  text-align: right;
}
.tooltip table td:first-child{
  text-align: left;
  width: 75%;
}
.tooltip table td:last-child{
  text-align: left;
}


