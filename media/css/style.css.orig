/*
 * Basic stuff
 */
body {
  font-family: 'PT Sans Narrow', sans-serif, 'Arial';
  font-size: 14px;
  background: #fafafa ; /* url('/media/img/all/containers.png') bottom right no-repeat */
  color: #343434;
  margin: 0;
  padding: 0;
}

a:active, a:focus { outline-style: none; -moz-outline-style:none; }
a, a:active, a:visited { color: #4f4f4f; }
a:hover { text-decoration: underline; }

p {
  font-family:'PT Sans Narrow',Helvetica,Arial;
  font-size: 14px;
  color:#343434;
}

h1, h2, h3, h4, h5, h6 {
  font-weight: normal;
}
h1 {font-size: 26px;}
h2 {font-size: 22px;}
h3 {font-size: 18px;}
h4 {font-size: 16px;}

dl dt {
  font-size: 16px;
  font-weight: bold;
  line-height: 1.2;
}
dl dd {
  margin: 0 0 10px 0;
  font-size: 14px;
  line-height: 1.4;
  color: #666;
}

.disabled {
  color: #bbb !important;
  cursor: default;
  pointer-events: none;
}

button {
  font-family: 'PT Sans Narrow',Helvetica,Arial;
  text-decoration: none;
  display: block;
  padding: 5px;
  border: 1px solid #DDD;
  text-align: center;

  -moz-border-radius:5px;
  -webkit-border-radius:5px;
  -o-border-radius:5px;
  border-radius:5px;

  background:#FFFFFF;
  background:-webkit-gradient(linear, 0% 0%, 0% 100%, from(#FFFFFF), to(#EEE));
  background:-moz-linear-gradient(0% 90% 90deg, #EEE, #FFF);

  -webkit-transition: all .4s ease-in-out;
  -moz-transition: all .4s ease-in-out;
  -o-transition: all .4s ease-in-out;
  transition: all .4s ease-in-out;
}
button:hover {
  border-color:#999;
  background:#aaa;
  background:-webkit-gradient(linear, 0% 0%, 0% 100%, from(#bbb), to(#aaa));
  background:-moz-linear-gradient(0% 90% 90deg, #bbb, #aaa);
}
button:active {
  background:#888;
  background:-webkit-gradient(linear, 0% 0%, 0% 100%, from(#888), to(#999));
  background:-moz-linear-gradient(0% 90% 90deg, #888, #999);
}

font.definition {
  transition: 0s border-bottom;
}

font.definition:hover{
  text-decoration:none;
  border-bottom:1px dotted gray;    
  transition-delay:0.5s;
}

/*
 * Page container
 */
#container {
  margin: 0 auto;
  width: 960px;
  margin: 0 auto;
  width: 1060px;
}

/*
 * Warning top of page
 */
.alert {
  border: solid 1px #EED3D7;
  background: #F2DEDE;
  color: #B94A48;
  font-size: 16px;
  padding: 10px 0;
}
.warning {
  border: solid 1px #f0c36d;
  background: #f9edbe;
  color: #a07631;
  font-size: 16px;
  padding: 10px 0;
}
.alert a,
.warning a {
  color: inherit;
}
.alert div,
.warning div{
  margin: auto;
  width: 1000px;
}
.alert h4,
.warning h4 {
  font-weight: bold;
  margin: 0;
}

/*
 * Header
 */
header {
  margin: 0 0 0 0;
}
header .logo {
  float: left;
  width: 30%;
}
header nav {
  float: right;
  width: 418px;
  text-align: center;
}
header nav ul {
  background: white;
  border: solid 1px #ccc;
  float: right;
  display: inline;
  height: 46px;
  margin: -2px 0px 0px 0px;
  padding: 11px 0px 10px 0px;
  
